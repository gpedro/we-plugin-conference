<div class="row">
  <div class="col-lg-12">
    {{we-action-create-btn model req conference.id}}
  </div>
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-body">
        <table class="table table-striped table-hover">
          <thead>
            <tr>
              <th we-model-attr="id">{{t 'id'}}</th>
              <th we-model-attr="name">{{t 'name'}}</th>
              <th>{{t 'actions'}}</th>
            </tr>
          </thead>
          <tbody>
            {{#each record}}
              <tr>
                <td>{{id}}</td>
                <td>{{name}}</td>
                <td>
                  {{#link-to 'cfroom.edit' conferenceId id class="btn btn-primary btn-sm"}}{{t 'Edit'}}{{/link-to}}
                  {{#link-to 'cfroom.delete' conferenceId id class="btn btn-default btn-sm"}}{{t 'Delete'}}{{/link-to}}
                </td>
              </tr>
            {{else}}
              <tr><td colspan="3" class="text-center">{{t 'conference.cfroom.empty'}}</td></tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
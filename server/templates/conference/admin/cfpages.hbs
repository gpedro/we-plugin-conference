<div class="row">
  <div class="col-lg-12 actions">{{we-action-create-btn model req conference.id}}</div>
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-body">
        <table class="table table-striped table-hover">
          <thead>
            <tr>
              <th we-model-attr="id">id</th>
              <th we-model-attr="title">title</th>
              <th we-model-attr="published">published</th>
              <th we-model-attr="createdAt">createdAt</th>
              <th we-model-attr="updatedAt">updatedAt</th>
              <th we-model-attr="weModelActions">actions</th>
            </tr>
          </thead>
          <tbody>
          {{#each record}}
            <tr>
              <td>{{id}}</td>
              <td>{{title}}</td>
              <td>{{published}}</td>
              <td>{{we-date createdAt}}</td>
              <td>{{we-date updatedAt}}</td>
              <td>
                {{we-action-view-btn 'cfpage' this ../req conferenceId id }}
                {{we-action-edit-btn 'cfpage' this ../req conferenceId id }}
                {{we-action-delete-btn 'cfpage' this ../req conferenceId id }}
              </td>
            </tr>
          {{/each}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-lg-12 actions">
  {{#link-to 'cfpage.create' conference.id class="btn btn-default"}}{{t 'Create'}}{{/link-to}}
  </div>
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-body">
        <table class="table table-striped table-hover">
          <thead>
            <tr>
              <th we-model-attr="id">id</th>
              <th we-model-attr="title">title</th>
              <th we-model-attr="published">published</th>
              <th we-model-attr="createdAt">createdAt</th>
              <th we-model-attr="updatedAt">updatedAt</th>
              <th we-model-attr="weModelActions">actions</th>
            </tr>
          </thead>
          <tbody>
          {{#each record}}
            <tr>
              <td>{{id}}</td>
              <td>{{title}}</td>
              <td>{{published}}</td>
              <td>{{we-date createdAt}}</td>
              <td>{{we-date updatedAt}}</td>
              <td>
               {{#link-to 'cfpage.edit' conferenceId id class='btn btn-primary btn-sm'}}{{t 'Edit'}}{{/link-to}}
                {{#link-to 'cfpage.delete' conferenceId id class='btn btn-default btn-sm'}}{{t 'Delete'}}{{/link-to}}
              </td>
            </tr>
          {{/each}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>